<h2>Editing Book</h2>
<%= render 'error', book: book %>

<%= form_with model: book do |f| %>
<p>Title</p>
<%= f.text_field :title %>

<p>Body</p>
<%= f.text_area :body %>

<div>
<%= f.submit "Update Book" %>
</div>
<% end %>

<%= link_to "Show", book_path(book.id) %> | <%= link_to "Back", books_path %>
